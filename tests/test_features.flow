# Test file for new Flow language features

# Test list literals
let my_list = [1, 2, 3, 4, 5]
print "Created list:", my_list

# Test built-in functions
let length = len("Hello, Flow!")
print "Length of string:", length

let sqrt_result = sqrt(16)
print "Square root of 16:", sqrt_result

let power_result = pow(2, 3)
print "2 to the power of 3:", power_result

# Test list functions
let numbers = [10, 20, 30]
let new_length = append(numbers, 40)
print "Appended to list:", numbers

let last_item = pop(numbers)
print "Popped item:", last_item
print "List after pop:", numbers

# Test range function
let range_list = range(5)
print "Range 0-4:", range_list

let range_list2 = range(2, 8)
print "Range 2-7:", range_list2

let range_list3 = range(0, 10, 2)
print "Range 0-10 step 2:", range_list3

# Test mathematical functions
let abs_value = abs(-42)
print "Absolute value of -42:", abs_value

let min_value = min(10, 5, 8, 3, 12)
print "Minimum value:", min_value

let max_value = max(10, 5, 8, 3, 12)
print "Maximum value:", max_value

let sum_value = sum([1, 2, 3, 4, 5])
print "Sum of list:", sum_value