# Comprehensive test for new Flow language features

# Test basic built-in functions
let length = len("Hello, Flow!")
print "Length of string:", length

let str_result = str(42)
print "String conversion:", str_result

# Test mathematical functions
let sqrt_result = sqrt(16)
print "Square root of 16:", sqrt_result

let power_result = pow(2, 3)
print "2 to the power of 3:", power_result

let abs_value = abs(-42)
print "Absolute value of -42:", abs_value

let min_value = min(10, 5, 8, 3, 12)
print "Minimum value:", min_value

let max_value = max(10, 5, 8, 3, 12)
print "Maximum value:", max_value

# Test list functions
let numbers = [1, 2, 3]
print "Original list:", numbers

append(numbers, 4)
print "After append:", numbers

let last_item = pop(numbers)
print "Popped item:", last_item
print "List after pop:", numbers

let sum_value = sum([1, 2, 3, 4, 5])
print "Sum of list:", sum_value

# Test range function
let range_list = range(5)
print "Range 0-4:", range_list

let range_list2 = range(2, 8)
print "Range 2-7:", range_list2

let range_list3 = range(0, 10, 2)
print "Range 0-10 step 2:", range_list3

# Test string functions
let words = "Hello,World,Flow"
let split_words = split(words, ",")
print "Split string:", split_words

let joined_words = join(" ", ["Hello", "Flow", "Language"])
print "Joined string:", joined_words

# Test list literals
let my_list = [1, 2, 3, 4, 5]
print "Created list:", my_list

# Test list indexing
let first_element = my_list[0]
print "First element:", first_element

# Test list index assignment
my_list[0] = 10
print "After assignment:", my_list

# Test user-defined functions
func add(a, b) {
    return a + b
}

let result = add(3, 4)
print "Add function result:", result

# Test time functions
let current_time = time()
print "Current time:", current_time