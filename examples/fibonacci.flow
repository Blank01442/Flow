# Fibonacci Example
print "Fibonacci Sequence"

func fibonacci(n) {
    if n < 2 {
        return n
    }
    return fibonacci(n - 1) + fibonacci(n - 2)
}

# Calculate and display first 10 Fibonacci numbers
let i = 0
while i < 10 {
    let fib = fibonacci(i)
    print "Fibonacci(", i, ") =", fib
    i = i + 1
}